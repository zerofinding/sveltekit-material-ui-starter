{
  "name": "sveltekit-material-ui-starter",
  "version": "0.0.1",
  "description": "A SvelteJS project (svelte-material-ui)",
  "author": "zerofinding <zerofinding.dev@gmail.com>",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "prepare": "npm run vite-workaround && npm run smui-theme-light && npm run smui-theme-dark",
    "vite-workaround": "node _helper/vite-workaround.js",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "dependencies": {
    "@fontsource/fira-mono": "^4.2.2",
    "@lukeed/uuid": "^2.0.0",
    "@smui/button": "^4.1.0",
    "@smui/drawer": "^4.1.0",
    "@smui/icon-button": "^4.1.0",
    "@smui/layout-grid": "^4.1.0",
    "@smui/list": "^4.1.0",
    "@smui/menu": "^4.1.0",
    "@smui/top-app-bar": "^4.1.0",
    "cookie": "^0.4.1"
  },
  "devDependencies": {
    "@material/theme": "^11.0.0",
    "@material/typography": "^11.0.0",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.7",
    "@sveltejs/kit": "next",
    "sass": "^1.32.11",
    "smui-theme": "^4.1.0",
    "svelte": "^3.37.0",
    "vite": "^2.2.3"
  },
  "type": "module"
}
